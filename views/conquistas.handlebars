<h1>Conquistas Cadastrados</h1>

<a href="/jogos/{{jogo.id}}/novaConquista">Cadastrar conquista.</a>

<table border="1">
  <!-- table row -->
  <tr>
    <!-- table head -->
    <td>Título:</td>
    <td>Descrição:</td>
  </tr>
  {{#each conquistas}}
    <tr>
      <td>{{this.titulo}}</td>
      <td>{{this.descricao}}</td>
    </tr>
  {{/each}}
</table>